{
	"name": "@ignatiusmb/styles",
	"version": "0.1.1",
	"description": "Essential stylesheets",
	"repository": "github:ignatiusmb/styles",
	"author": "Ignatius Bagus",
	"type": "module",
	"scripts": {
		"check": "prettier -c . --plugin=prettier-plugin-sort-package-json",
		"format": "prettier -w . --plugin=prettier-plugin-sort-package-json"
	},
	"exports": {
		"./core.css": "./core.css",
		"./package.json": "./package.json"
	},
	"files": [
		"core.css"
	],
	"packageManager": "pnpm@8.7.1",
	"prettier": "mauss/prettier.json",
	"keywords": [
		"essential",
		"stylesheets",
		"css"
	],
	"devDependencies": {
		"mauss": "^0.7.2",
		"prettier": "^3.0.3",
		"prettier-plugin-sort-package-json": "^0.2.0"
	}
}
