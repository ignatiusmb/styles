{
	"name": "@ignatiusmb/styles",
	"version": "0.1.1",
	"description": "Essential stylesheets",
	"repository": "github:ignatiusmb/styles",
	"author": "Ignatius Bagus",
	"type": "module",
	"scripts": {
		"check": "pnpm check:style",
		"check:style": "prettier -c . --plugin=prettier-plugin-sort-package-json",
		"pub:format": "prettier -w . --plugin=prettier-plugin-sort-package-json",
		"prepublishOnly": "pnpm pub:format"
	},
	"exports": {
		"./core.css": "./core.css",
		"./package.json": "./package.json"
	},
	"packageManager": "pnpm@8.6.11",
	"prettier": "mauss/prettier.json",
	"keywords": [
		"essential",
		"stylesheets",
		"css"
	],
	"devDependencies": {
		"mauss": "^0.7.0",
		"prettier": "^3.0.1",
		"prettier-plugin-sort-package-json": "^0.1.1"
	}
}
